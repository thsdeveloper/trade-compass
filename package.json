{
  "name": "trade-compass",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:front": "pnpm --filter front dev",
    "dev:back": "pnpm --filter back dev",
    "build": "pnpm -r build",
    "build:front": "pnpm --filter front build",
    "build:back": "pnpm --filter back build",
    "lint": "pnpm -r lint",
    "test": "pnpm --filter back test",
    "generate-data": "pnpm --filter back generate-data"
  }
}
